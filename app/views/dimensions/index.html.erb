<h1>DIMENSIONS</h1>
<%= link_to 'New Dimension', new_dimension_path, class: "btn btn-default" %>
<table class="table">
  <thead>
    <tr>
      <th>Board</th>
      <th>Length</th>
      <th>Width</th>
      <th>Thickness</th>
      <th>Board</th>
      <th>Volume</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dimensions.each do |dimension| %>
      <tr>
        <td><%= dimension.board.try(:board_name) %></td>
        <td><%= dimension.length %></td>
        <td><%= dimension.width %></td>
        <td><%= dimension.thickness %></td>
        <td><%= dimension.board_id %></td>
        <td><%= dimension.volume %></td>
        <td><%= link_to 'Show', dimension %></td>
        <td><%= link_to 'Edit', edit_dimension_path(dimension) %></td>
        <td><%= link_to 'Destroy', dimension, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


